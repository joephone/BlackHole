<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.transcendence.wan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_deer"
        android:label="@string/app_wan_name"
        android:roundIcon="@mipmap/ic_deer"
        android:supportsRtl="true"
        android:theme="@style/WanAppTheme">
        <activity android:name=".main.WanSplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.WanMainActivity" />
    </application>

</manifest>